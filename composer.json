{
  "name": "provydon/laravel-scale",
  "description": "Scale your Laravel app. A set of libraries: Laravel Octane (FrankenPHP), Docker, and a stateless web + worker setup for Render, Fly.io, Railway.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "octane",
    "docker",
    "frankenphp",
    "render",
    "scale",
    "production"
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "laravel/octane": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "LaravelScale\\LaravelScale\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelScale\\LaravelScale\\LaravelScaleServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "version": "1.10.0",
  "require-dev": {
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^11.0"
  },
  "scripts": {
    "pint": "pint",
    "test": "phpunit"
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelScale\\LaravelScale\\Tests\\": "tests/"
    }
  }
}